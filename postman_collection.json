{
  "info": {
    "name": "Todo-App Starter",
    "_postman_id": "todo-app-starter",
    "description": "Colecci\u00f3n m\u00ednima para demo",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Register",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "url": {
          "raw": "http://localhost:3001/api/auth/register",
          "protocol": "http",
          "host": [
            "localhost"
          ],
          "port": "3001",
          "path": [
            "api",
            "auth",
            "register"
          ]
        },
        "body": {
          "mode": "raw",
          "raw": "{\"email\": \"a@a.com\", \"password\": \"12345678\"}"
        }
      }
    },
    {
      "name": "Login",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "url": {
          "raw": "http://localhost:3001/api/auth/login",
          "protocol": "http",
          "host": [
            "localhost"
          ],
          "port": "3001",
          "path": [
            "api",
            "auth",
            "login"
          ]
        },
        "body": {
          "mode": "raw",
          "raw": "{\"email\": \"a@a.com\", \"password\": \"12345678\"}"
        }
      }
    },
    {
      "name": "Tasks - List",
      "request": {
        "method": "GET",
        "header": [
          {
            "key": "Authorization",
            "value": "Bearer {{accessToken}}"
          }
        ],
        "url": {
          "raw": "http://localhost:3002/api/tasks",
          "protocol": "http",
          "host": [
            "localhost"
          ],
          "port": "3002",
          "path": [
            "api",
            "tasks"
          ]
        }
      }
    }
  ],
  "variable": [
    {
      "key": "accessToken",
      "value": ""
    }
  ]
}